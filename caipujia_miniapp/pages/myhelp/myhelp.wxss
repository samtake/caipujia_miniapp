/* pages/myhelp/myhelp.wxss */
.container{
  position:absolute;
  flex-direction:column;
  justify-content:flex-start;
  background-color:#F2f2f2;

}
.search-wrap {
    display: flex;
    align-items: center;
    width: 100%;
    height: 90rpx;
    color: #fff;
    background:white ;
  }
  .search {
    flex: 1;
    margin: 0 24rpx;
    border-radius: 30rpx;
    overflow: hidden;
  }

  .helplist{
    width: 100%;
      margin-top: 20rpx;
  }
  .help-item{
    background-color: white;
    padding: 0 30rpx;
    line-height: 90rpx;
    font-size: 28rpx;
    margin-bottom: 2rpx;
  }
.fixedb{
  position: fixed;
  bottom: 0;
  left:0;
  width: 100%;
  background-color: white;
  line-height: 80rpx;
  font-size: 28rpx;
}